            <TextField fx:id="place" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label prefHeight="17.0" prefWidth="131.0" text="Name &amp; Title" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label prefHeight="17.0" prefWidth="130.0" text="Location" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="submitBtn" mnemonicParsing="false" onAction="#clickSubmit" text="Submit" GridPane.columnIndex="3" GridPane.rowIndex="3" />
            <Label fx:id="errorSubmit" prefHeight="60.0" prefWidth="220.0" text="One or more entries was left empty!!" visible="false" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </GridPane>
   </children>
