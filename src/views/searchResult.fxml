<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<HBox fx:id="root" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets left="10.0"/>
    </padding>
    <VBox>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Label fx:id="locationIcon" prefHeight="30.0" prefWidth="30.0" stylesheets="@../styles/main-menu.css"/>
    </VBox>
    <VBox prefWidth="250.0" styleClass="location-border" stylesheets="@../styles/main-menu.css">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Label fx:id="locationLabel" prefHeight="30.0" prefWidth="230.0" stylesheets="@../styles/main-menu.css"/>
    </VBox>
</HBox>
