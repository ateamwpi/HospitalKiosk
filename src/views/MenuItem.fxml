<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="root" onMouseClicked="#onPressed" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets left="10.0" />
    </padding>
     <Label fx:id="menuIcon" prefHeight="30.0" prefWidth="30.0" stylesheets="@../styles/main-menu.css" />
     <Label fx:id="menuLabel" prefHeight="30.0" prefWidth="230.0" styleClass="alert-body" stylesheets="@../styles/style.css" text="text">
      <padding>
         <Insets left="32.0" />
      </padding>
   </Label>
   <opaqueInsets>
      <Insets left="16.0" />
   </opaqueInsets>
</HBox>
